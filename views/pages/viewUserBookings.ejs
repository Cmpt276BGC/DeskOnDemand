<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title>Desks On Demand</title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" href="/stylesheets/mainSS.css">
    </head>
    <body>
        <div class="background">
        <h3 class="header">View User Bookings</h3>
            <form method="POST" action="/users/admindash/deleteUserBooking">
                <% viewUserBookingsQueryResults.forEach(function(r) { %>
                    <ul>
                        <li> <%= r.tableid %>: <%= r.fromdate %> <b>to</b> <%= r.todate %></li> 
                        <li>Delete this booking ID number <input type="submit" style="background-color: white; color: black; border: 1px solid black;" value="<%= r.uniqueid %>" name="deleteID" value="deleteID"></li>
                    </ul>
                    
                <% }); %><br>
                <input type="hidden" value="<%= emailJSON.uemail %>" name="uemail" id="uemail">
            </form>        
        <form method="GET" action="/db">
            <input type="submit" value="Return">
        </form>

        <form method="GET" action="/users/admindash">
            <input type="submit" value="Return to admin dash">
        </form>
        
        <form method="GET" action="/users/logout">
            <input type="submit" value="Log Out">
        </form>
        
        <script type="text/javascript" src="/public/adminbutton.js">
        </script>
        </div>
    </body>
</html>