<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title>Users:</title>
    <link rel="stylesheet" href="/public/stylesheets/mainSS.css">
  </head>

  <div class="background">
    <body>
      <div class="container">
        <h2>Database Results</h2>
        <form method="GET" action="/users/admindash/manageUsers">
          <input type="submit" value="Return to Manage Users">
        </form>
        <% results.forEach(function(r) { %>
          <form method="POST" action="/users/admindash/viewUserBookings">
            <ul>
                <li><b>Name: </b><%= r.fname %> <%= r.lname %> <b>Email:</b> <%= r.uemail %> <b>Admin:</b> <%=r.admin%> <b>ID:</b> <%= r.id %> | View bookings for <input type="submit" style="background-color: white; color: black; border:1px solid black;" value="<%= r.uemail %>" name="viewBookingsEmail" id="viewBookingsEmail"></li><br>
              <% }); %>
            </ul>
          </form>
        </div>
    </body>
  </div>
</html>