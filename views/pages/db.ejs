<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title>Users:</title>
    <link rel="stylesheet" href="/public/stylesheets/mainSS.css">
  </head>

  <div class="background">
    <body>
      <div class="container">
        <h2>Database Results</h2>
        <form method="GET" action="/users/admindash/manageUsers">
          <input type="submit" value="Return to Manage Users">
        </form>
          <ul>
            <% results.forEach(function(r) { %>
              <li>Name: <%= r.fname %> <%= r.lname %> Email: <%= r.uemail %> Admin: <%=r.admin%> ID: <%= r.id %></li>
            <% }); %>
          </ul>
          <h2>End of List</h2>
        </div>
    </body>
  </div>
</html>