<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <% rows.forEach(function(r) { %>
        <p>Unique ID: <%= r.uniqueid %></p>
        <form  method="POST" action="/cancelBooking">
        <label for="cancelbyID">Please type in the unique id to cancel this booking: </label>
        <input type="text" id="cancelbyID" name="cancelbyID" placeholder="Delete by id">
        <input type="submit" class="delete" name="remove" value="Cancel Booking">
        </form>
    <% }); %>
</body>
</html>